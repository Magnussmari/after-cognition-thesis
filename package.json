  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "supabase": "^1.142.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
